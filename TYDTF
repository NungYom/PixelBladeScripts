titleLabel.TextXAlignment = Enum.TextXAlignment.Left
titleLabel.Parent = mainFrame

-- ‡∏õ‡πâ‡∏≤‡∏¢‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞
local statusLabel = Instance.new("TextLabel")
statusLabel.Size = UDim2.new(1, -20, 0, 25)
statusLabel.Position = UDim2.new(0, 10, 0, 35)
statusLabel.BackgroundTransparency = 1
statusLabel.Text = "‡∏™‡∏ñ‡∏≤‡∏ô‡∏∞: ‡∏´‡∏¢‡∏∏‡∏î‡∏ó‡∏≥‡∏á‡∏≤‡∏ô"
statusLabel.TextColor3 = Color3.new(1, 1, 1)
statusLabel.Font = Enum.Font.SourceSans
statusLabel.TextSize = 18
statusLabel.TextXAlignment = Enum.TextXAlignment.Left
statusLabel.Parent = mainFrame

-- ‡∏õ‡∏∏‡πà‡∏° Start Loop
local btnStartLoop = Instance.new("TextButton")
btnStartLoop.Size = UDim2.new(1, -20, 0, 30)
btnStartLoop.Position = UDim2.new(0, 10, 0, 70)
btnStartLoop.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
btnStartLoop.TextColor3 = Color3.new(1, 1, 1)
btnStartLoop.Font = Enum.Font.SourceSansBold
btnStartLoop.TextSize = 18
btnStartLoop.Text = "Start Loop [OFF]"
btnStartLoop.Parent = mainFrame

local loopRunning = false
btnStartLoop.MouseButton1Click:Connect(function()
	loopRunning = not loopRunning
	btnStartLoop.Text = "Start Loop" .. (loopRunning and " [ON]" or " [OFF]")
	if loopRunning then
		statusLabel.Text = "Start Loop: ‡∏Å‡∏≥‡∏•‡∏±‡∏á‡∏ó‡∏≥‡∏á‡∏≤‡∏ô..."
		coroutine.wrap(function()
			while loopRunning do
				for i = 1, 11 do
					local args = {
						"\233\162\134\229\143\150\229\156\168\231\186\191\229\165\150\229\138\177",
						i
					}
					pcall(function()
						game:GetService("ReplicatedStorage"):WaitForChild("Msg"):WaitForChild("RemoteFunction"):InvokeServer(unpack(args))
					end)
					task.wait(1)
				end
			end
			statusLabel.Text = "Start Loop: ‡∏´‡∏¢‡∏∏‡∏î‡πÅ‡∏•‡πâ‡∏ß"
		end)()
	else
		statusLabel.Text = "Start Loop: ‡∏´‡∏¢‡∏∏‡∏î‡πÅ‡∏•‡πâ‡∏ß"
	end
end)
